corr <- function(directory, threshold = 0){
source("complete.R")
complete_data <- complete(directory)
file_names <- list.files(directory, full.names = TRUE)
cr <- numeric(0)
for (i in 1:nrow(complete_data)) {
new_df <- na.omit(read.csv(file_names[complete_data$id[i]]))
if (sum(complete.cases(new_df) > threshold)) {
##compute correlation cor(df$nitrate, df$sulfate..)
##build the whole resultset with c()
cr <- c(cr, cor(new_df$sulfate, new_df$nitrate))
}
}
summary(cr)
}
